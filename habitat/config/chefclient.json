{
  "camsa": {
    "firstname": "{{cfg.attributes.camsa.firstname}}",
    "lastname": "{{cfg.attributes.camsa.lastname}}",
    "emailaddress": "{{cfg.attributes.camsa.emailaddress}}",
    "username": "{{cfg.attributes.camsa.username}}",
    "password": "{{cfg.attributes.camsa.password}}",
    "license": "{{cfg.attributes.camsa.license}}",
    "gdpr": {{cfg.attributes.camsa.gdpr}},
    "storage_account": {
      "name": "{{cfg.attributes.camsa.sa.name}}",
      "container_name": "{{cfg.attributes.camsa.sa.container_name}}",
      "access_key": "{{cfg.attributes.camsa.sa.access_key}}"
    },
    "managed_app": {{cfg.attributes.camsa.managed_app}},
    "azure_functions": {
      "camsa": {
        "url": "{{cfg.attribuites.camsa.azure_functions.camsa.url}}",
        "apikey": "{{cfg.attribuites.camsa.azure_functions.camsa.apikey}}"
      },
      "central": {
        "url": "{{cfg.attribuites.camsa.azure_functions.central.url}}",
        "apikey": "{{cfg.attribuites.camsa.azure_functions.central.apikey}}"        
      }
    },
    "chefserver": {
      "org": {
        "name": "{{cfg.attributes.chefserver.org.name}}",
        "description": "{{cfg.attributes.chefserver.org.description}}"
      }
    },
    "log_analytics": {
      "enabled": {{cfg.attributes.log_analytics.enabled}}
    }
  }
}